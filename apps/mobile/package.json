{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "expo start",
    "build": "expo export --platform web && npm run copy-web-files",
    "vercel-build": "expo export --platform web && npm run copy-web-files",
    "copy-web-files": "mkdir -p dist/web && mkdir -p dist/web/fonts && cp web/*.html web/*.js web/*.css dist/web/ && cp web/fonts/*.ttf dist/web/fonts/"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@supabase/supabase-js": "^2.50.3",
    "buffer": "^6.0.3",
    "expo": "^53.0.19",
    "expo-audio": "^0.4.8",
    "expo-av": "~15.1.7",
    "expo-camera": "^16.1.10",
    "expo-file-system": "^18.1.11",
    "expo-font": "^13.3.2",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-media-library": "~17.1.7",
    "expo-sharing": "~13.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-video": "^2.2.2",
    "expo-video-thumbnails": "^9.1.3",
    "lucide-react-native": "^0.525.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}